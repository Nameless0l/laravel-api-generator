{
    "name": "nameless/laravel-api-generator",
    "description": "A powerful Laravel API generator that automatically creates a complete API structure including models, controllers, services, DTOs, policies, resources, factories, migrations, Seeders and more with a single command.",

    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Mbassi Loic Aron",
            "email": "loicmbassi5@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "dedoc/scramble": "^0.11.31",
        "laravel/framework": "^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "nameless\\CodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nameless\\CodeGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "nameless\\CodeGenerator\\Providers\\CodeGeneratorServiceProvider"
            ]
        }
    },
    "include-path": [
        "vendor\\nameless/laravel-api-generator\\stubs"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
